public static class Program
{
	public static void Main()
	{	
	    int totalIncome = 0;
		int totalExpense = 0;
		
		Console.Write("Enter the number of income amounts: ");
		int numIncome = int.Parse(Console.ReadLine());
		Console.Write("Enter the expense amounts: ");
		int numExpense = int.Parse(Console.ReadLine());
		
		int i = 1;
		while(i <= 5)
		{
		  Console.Write("Enter income amount: ");
		  int income = int.Parse(Console.ReadLine());
		  totalIncome = income; i++;
		}
			
		  int n = 1;
		  while(n <= 3);
		  {
		    Console.Write("Enter expense amount: ");
			int expense = int.Parse(Console.ReadLine());
			totalExpense = expense; n++;
		  }
			//calculate profit
		    int profit = totalIncome - totalExpense;
			//Display the results
			Console.WriteLine("totalIncome:" + totalIncome);
			Console.WriteLine($"profit:" + profit);
  }
}
